public class Dispositivo {
    // Se define la clase dispositivo
    String nombreDispositivo;
    String categoria;
    float precio_venta;
    float porcentaje_IVA;
    String codigo;
    int unidadesDisponibles;
    int umbralMinimo;

    public Dispositivo(String nombreDispositivo, String categoria, float precio_venta, float porcentaje_IVA, String codigo, int unidadesDisponibles, int umbralMinimo) {
        this.nombreDispositivo = nombreDispositivo;
        this.categoria = categoria;
        this.precio_venta = precio_venta;
        this.porcentaje_IVA = porcentaje_IVA;
        this.codigo = codigo;
        this.unidadesDisponibles = unidadesDisponibles;
        this.umbralMinimo = umbralMinimo;
    }
}
